{
  "app": {
    "actions": [
      {
        "_type": "Function",
        "attributes": {
          "_target": "app",
          "functionName": "location.beginTracking"
        },
        "on": "willEnterForeground"
      }
    ],
    "attributes": {
      "view.index": "viewRestaurantList.json",
      "debug.layout.enabled": false,
      "logging.datasource.enabled": true,
      "logging.level": "verbose",
      "mode": "verbose",
      "navigationBar.enabled": false,
      "apigee.org": "grewis",
      "apigee.app": "sandbox",
      "searchTerm": "restaurants",
      "drawerController.animation": "slide",
      "drawerController.animation.parallaxFactor": 2,
      "drawerController.l.max.w": "{{[[device.screen.w]] - 100}}",
      "drawerController.l.url": "viewMenu.json",
      "drawerController.r.max.w": "{{[[device.screen.w]] - 50}}",
      "drawerController.r.url": "viewMenu.json",
      "drawerController.shadow.enabled": false
    },
    "datasources": [
      {
        "_id": "dataSearchHistory-old",
        "_type": "JSON",
        "actions": [
          {
            "_type": "Refresh",
            "attributes": {
              "_target": "tableviewHistory"
            },
            "on": [
              "success",
              "fail"
            ]
          }
        ],
        "attributes": {
          "autoLoad.enabled": false,
          "baseUrl": "searchHistory.json",
          "data.basepath": "entities",
          "http_method": "GET"
        }
      },
      {
        "_id": "dataSearchHistoryAppend-old",
        "_type": "JSON",
        "actions": [
          {
            "_type": "Refresh",
            "attributes": {
              "_target": "tableviewHistory"
            },
            "on": [
              "success",
              "fail"
            ]
          }
        ],
        "attributes": {
          "autoLoad.enabled": false,
          "baseUrl": "searchHistoryAppend.json",
          "data.basepath": "entities",
          "http_method": "GET"
        }
      }
    ]
  }
}
{
  "$view": {
    "attributes": {
      "_id": "menu",
      "statusBar.style": "hidden",
      "bg.color": "#ffffff"
    },
    "actions": [],
    "controls": [
      {
        "_id": "tableviewTest",
        "_type": "TableView",
        "attributes": {
          "cell.size.h": 50,
          "cell.size.w": "100%",
          "datasource.id": "dataTest",
          "data.basepath": "entities",
          "padding.top": 20,
          "size.w": "{{[[$device.screen.w]] - 100}}",
          "size.h": "100%",
          "separator.style": "default"
        },
        "controls": [
          {
            "_type": "Text",
            "attributes": {
              "bg.color": "#ffffff",
              "color": "#FF4300",
              "size.h": "100%",
              "size.w": "100%",
              "text":"[[$dataRow.index]]"
            }
          }
        ]
      },
      {
        "_type": "Text",
        "actions": [
          {
            "_type": "Function",
            "attributes": {
              "_target": "dataTest",
              "functionName": "modify_response"
            },
            "set": {
              "top_level_container": "entities",
              "parse_json_as_object": true,
              "modify.type": "append",
              "json_to_append": {
                "key2": "value2"
              }
            },
            "enabled": true,
            "on": "touchUp"
          },
          {
            "_type": "Load",
            "attributes": {
              "_target": "tableviewTest"
            },
            "enabled": true,
            "on": "touchUp"
          }
        ],
        "attributes": {
          "layoutType": "float",
          "color": "#ffffff",
          "align.v": "bottom",
          "size.h": 50,
          "size.w": "100%",
          "bg.color": "666666",
          "font": "HelveticaNeue-Light:22",
          "text.align":"center",
          "text": "Add Row"
        }
      }
    ],
    "datasources": [
      {
        "_id": "dataTest",
        "_type": "JSON",
        "actions": [
          {
            "_type": "Alert",
            "attributes": {
              "message": "[[$self.response.raw]]"
            },
            "enabled": false,
            "on": "success,error"
          },
          {
            "_type": "Refresh",
            "attributes": {
              "_target": "tableviewTest"
            },
            "enabled": true,
            "on": "success"
          },
          {
            "_type": "Alert",
            "attributes": {
              "title": "[[$self.response.raw]]"
            },
            "enabled": false,
            "on": "success"
          }
        ],
        "attributes": {
          "autoLoad.enabled": true,
          "baseUrl": "/searchHistory.json"
        }
      }
    ]
  }
}
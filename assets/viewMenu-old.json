{
  "view": {
    "actions": [],
    "attributes": {
      "_id": "viewMenu",
      "bg.color": "#ffffff",
      "statusBar.style": "hidden"
    },
    "controls": [
      {
        "_id": "layoutMain",
        "_type": "Layout",
        "attributes": {
          "bg.color": "#ffffff",
          "layoutType": "absolute",
          "size.h": "100%",
          "size.w": "100%",
          "padding.bottom": 4
        },
        "controls": [
          {
            "_id": "layoutInput",
            "_type": "Layout",
            "attributes": {
              "layoutType": "absolute",
              "margin.top": 14,
              "size.h": 46,
              "size.w": "{{[[device.screen.w]] - 100}}"
            },
            "controls": [
              {
                "_id": "layoutSearch",
                "_type": "Layout",
                "attributes": {
                  "align.h": "center",
                  "align.v": "middle",
                  "layoutFlow": "horizontal",
                  "layoutType": "absolute",
                  "size.h": 38,
                  "size.w": "{{[[device.screen.w]] - 110}}",
                  "border.size": 1,
                  "border.color": "FF4300"
                },
                "controls": [
                  {
                    "_id": "inputSearch",
                    "_type": "TextInput",
                    "actions": [
                      {
                        "_type": "Modify",
                        "attributes": {
                          "_target": "session"
                        },
                        "enabled": true,
                        "on": "textChanged",
                        "set": {
                          "searchTerm": "[[$self.text]]"
                        }
                      },
                      {
                        "_type": "Function",
                        "attributes": {
                          "_target": "dataTest",
                          "functionName": "modify_response"
                        },
                        "set": {
                          "top_level_container": "entities",
                          "parse_json_as_object": true,
                          "modify.type": "append",
                          "json_to_append": {
                            "term": "[[inputSearch.text]]"
                          }
                        },
                        "enabled": true,
                        "on": "returnKeyPressed"
                      },
                      {
                        "_type": "Alert",
                        "attributes": {
                          "message": "[[dataTest.response.raw]]"
                        },
                        "enabled": true,
                        "on": "returnKeyPressed"
                      },
                      {
                        "_type": "Refresh",
                        "attributes": {
                          "_target": "searchHistoryCollection"
                        },
                        "enabled": true,
                        "on": "returnKeyPressed"
                      },
                      {
                        "_type": "Navigate",
                        "attributes": {
                          "to": "viewRestaurantList.json"
                        },
                        "enabled": false,
                        "on": "returnKeyPressed"
                      }
                    ],
                    "attributes": {
                      "bg.color": "#ffffff",
                      "clearOnFocus.enabled": true,
                      "font": "HelveticaNeue-Light:18",
                      "keyboard.appearance": "dark",
                      "margin.left": 4,
                      "padding.left": 8,
                      "placeholder.text": "Enter a search term...",
                      "size.h": "100%",
                      "size.w": "{{[[device.screen.w]] - 130}}"
                    }
                  },
                  {
                    "_id": "searchBtn",
                    "_type": "Text",
                    "actions": [
                      {
                        "_type": "Navigate",
                        "attributes": {
                          "to": "viewRestaurantList.json"
                        },
                        "enabled": true,
                        "on": "touchUp"
                      },
                      {
                        "_type": "Function",
                        "attributes": {
                          "_target": "inputSearch",
                          "functionName": "dismissKeyboard"
                        },
                        "enabled": true,
                        "on": "touchUp"
                      }
                    ],
                    "attributes": {
                      "align.h": "right",
                      "align.v": "middle",
                      "color": "#FF4300",
                      "font": "FontAwesome:20",
                      "text": "fa-search",
                      "layoutType": "float",
                      "margin.left": 15,
                      "padding.top": 6,
                      "size.h": 28,
                      "size.w": 28,
                      "visible": true
                    }
                  },
                  {
                    "_type": "Text",
                    "attributes": {
                      "color": "#000000",
                      "font": "HelveticaNeue-Light:22",
                      "margin.top": 120,
                      "margin.left": 20,
                      "text": "test"
                    }
                  }
                ]
              }
            ]
          },
          {
            "_id": "layoutHistory",
            "_type": "Layout",
            "attributes": {
              "align.h": "center",
              "align.v": "top",
              "layoutType": "absolute",
              "padding": 75,
              "padding.left": 6,
              "padding.right": 6,
              "margin.top": 60,
              "size.w": "100%"
            },
            "controls": [
              {
                "_id": "searchHistoryCollection",
                "_type": "Table",
                "attributes": {
                  "autofill.w": true,
                  "cell.size.h": 50,
                  "cell.size.w": "100%",
                  "datasource.id": "dataTest",
                  "data.basepath": "entities",
                  "enabled": true,
                  "autofill.h": true,
                  "margin.top": 70,
                  "separator.style": "none"
                },
                "controls": [
                  {
                    "_type": "Text",
                    "attributes": {
                      "color": "#6c6c6c",
                      "font": "HelveticaNeue-Light:18",
                      "margin.left": 20,
                      "text": "[[dataRow.term]]"
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ],
    "datasources": [
      {
        "_id": "dataTest",
        "_type": "JSON",
        "actions": [
          {
            "_type": "Alert",
            "attributes": {
              "message": "[[$self.response.raw]]"
            },
            "enabled": false,
            "on": "success,error"
          },
          {
            "_type": "Refresh",
            "attributes": {
              "_target": "tableviewTest"
            },
            "enabled": true,
            "on": "success"
          },
          {
            "_type": "Alert",
            "attributes": {
              "title": "[[$self.response.raw]]"
            },
            "enabled": false,
            "on": "success"
          }
        ],
        "attributes": {
          "autoLoad.enabled": true,
          "baseUrl": "/searchHistory.json"
        }
      }
    ]
  }
}